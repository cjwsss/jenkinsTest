pipeline {
    agent any

    parameters {
      string name: 'myName', trim: true
    }

    environment {
            myName = "${params.myName}"
        }

    stages {
        stage('hello') {
            steps {
                script {
                    sh '''
                        #!/bin/bash
                        ./test.sh $myName
                    '''
                }
            }
        }
    }
}
